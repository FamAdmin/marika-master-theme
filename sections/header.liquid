            </li>
          {% endif %}
        {% endcapture %}


        {% if section.settings.logo_position == "left" or section.settings.menu_position == "block" %}

          <div class="nav">

            <!-- Rearrange top half of nav and add to sticky_nav 7/2/18 -->
            <div class="nav main_nav duplicate_main_nav" style="width: 100%; padding-top: 5px;">
              <ul class="menu align_left clearfix" style="float: left; padding: 0; text-align: left;width: 35%; margin: 0;padding-right: 30px;">
                <p class="nav-free-shipping">
                  Free Shipping On All Orders!
                </p>
              </ul>
              {{ logo }}
