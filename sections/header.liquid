    },
	{
      "type": "text",
      "id": "promo_details_text",
      "label": "Promo Details Text"
    },
	{
      "type": "url",
      "id": "promo_details_url",
      "label": "Promo Details URL"
    },
    {
      "type": "number",
      "id": "promo_speed",
      "label": "Promo slider speed (ms)",
      "default": 5000
    },
    {
      "type": "color",
      "id": "promo_banner_color",
      "label": "Announcement banner background color",
      "default": "#f2f2f2"
    },
    {
      "id": "promo_banner_text_color",
      "type": "color",
      "label": "Promo Banner Text Color",
      "default": "#020202"
    },
	{
      "id": "promo_banner_font_size_small_screen",
      "type": "range",
      "label": "Promo Banner Font Size Small Screen",
      "default": 8,
      "min": 6,
      "max": 16,
      "step": 1
    },
    {
      "type": "checkbox",
      "id": "social_icons_header",
      "label": "Show social media header icons",
      "default": true
    },
