    {% comment %}
      Login Page NoIndex/NoFollow meta tag render
    {% endcomment %}

    {%- if hso_is_meta_used_before == false and hso_noindex_template_name == 'customers/login' and shop.metafields.hso_noindex.bulk_settings.value.loginPage.noIndex == true and shop.metafields.hso_noindex.bulk_settings.value.loginPage.noFollow == true -%}
      <meta name="robots" content="noindex,nofollow">
      {%- assign hso_is_meta_used_before = true -%}
    {%- elsif hso_is_meta_used_before == false and hso_noindex_template_name == 'customers/login' and shop.metafields.hso_noindex.bulk_settings.value.loginPage.noIndex == true -%}
