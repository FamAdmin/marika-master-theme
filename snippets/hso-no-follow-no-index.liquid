      {%- assign hso_is_meta_used_before = true -%}
    {%- endif -%}

    
    {% comment %}
      Out of Stock product NoIndex/NoFollow meta tag render
    {% endcomment %}

    {%- if hso_is_meta_used_before == false and hso_noindex_template_name == 'product' and product.available == false and shop.metafields.hso_noindex.bulk_settings.value.outOfStockProductsPage.noIndex == true and shop.metafields.hso_noindex.bulk_settings.value.outOfStockProductsPage.noFollow == true -%}
