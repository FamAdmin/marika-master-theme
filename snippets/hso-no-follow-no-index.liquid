      {%- assign hso_is_meta_used_before = true -%}
    {%- elsif hso_is_meta_used_before == false and hso_IsPaginationPage == true and shop.metafields.hso_noindex.bulk_settings.value.paginationPage.noIndex == true -%}
      <meta name="robots" content="noindex">
      {%- assign hso_is_meta_used_before = true -%}
    {%- elsif hso_is_meta_used_before == false and hso_IsPaginationPage == true and shop.metafields.hso_noindex.bulk_settings.value.paginationPage.noFollow == true -%}
      <meta name="robots" content="nofollow">
      {%- assign hso_is_meta_used_before = true -%}
    {%- endif -%}


    {%- if hso_is_meta_used_before == false and hso_noindex_template_name == 'product' and product.metafields.seo.hidden == 2 -%}
    <meta name="robots" content="noindex,nofollow">
    {%- assign hso_is_meta_used_before = true -%}
    {%- endif -%}

    {%- if hso_is_meta_used_before == false and hso_noindex_template_name == 'product' and product.metafields.seo.hidden == 3 -%}
    <meta name="robots" content="noindex">
    {%- assign hso_is_meta_used_before = true -%}
    {%- endif -%}

    {%- if hso_is_meta_used_before == false and hso_noindex_template_name == 'collection' and collection.metafields.seo.hidden == 2 -%}
    <meta name="robots" content="noindex,nofollow">
