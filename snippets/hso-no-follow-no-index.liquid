    {% endcomment %}

    {%- if hso_is_meta_used_before == false and hso_IsPaginationPage == true and shop.metafields.hso_noindex.bulk_settings.value.paginationPage.noIndex == true and shop.metafields.hso_noindex.bulk_settings.value.paginationPage.noFollow == true -%}
