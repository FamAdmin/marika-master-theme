    {% comment %}
      Register Page NoIndex/NoFollow meta tag render
    {% endcomment %}

    {%- if hso_is_meta_used_before == false and hso_noindex_template_name == 'customers/register' and shop.metafields.hso_noindex.bulk_settings.value.registerPage.noIndex == true and shop.metafields.hso_noindex.bulk_settings.value.registerPage.noFollow == true -%}
      <meta name="robots" content="noindex,nofollow">
